#include <stdio.h>

int main() {
    int Arr[100];
    int n;
    printf("Enter the no elements in array ");
    scanf("%d",&n);
    printf("Enter the elements of array:");
    for(int i=0;i<n;i++){
        scanf("%d",&Arr[i]);
    }
    for(int i=n-1;i>=0;i--){
        printf("%d\t" ,Arr[i]);
    }
    return 0;
}